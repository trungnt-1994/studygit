From: longnvcmc <longnvcmc>
Date: Fri, 2 Feb 2018 02:20:04 +0000
Subject: [PATCH] debian-test

---
 whichman-2.4/debian/tests/control         |  2 ++
 whichman-2.4/debian/tests/whichman_script | 30 ++++++++++++++++++++++++++++++
 2 files changed, 32 insertions(+)
 create mode 100644 whichman-2.4/debian/tests/control
 create mode 100644 whichman-2.4/debian/tests/whichman_script

diff --git a/whichman-2.4/debian/tests/control b/whichman-2.4/debian/tests/control
new file mode 100644
index 0000000..5daccf0
--- /dev/null
+++ b/whichman-2.4/debian/tests/control
@@ -0,0 +1,2 @@
+Tests: whichman_script
+Depends: @
diff --git a/whichman-2.4/debian/tests/whichman_script b/whichman-2.4/debian/tests/whichman_script
new file mode 100644
index 0000000..fc99475
--- /dev/null
+++ b/whichman-2.4/debian/tests/whichman_script
@@ -0,0 +1,30 @@
+#!/bin/sh
+
+set -e
+
+#Checking for autopkgtest dir, where tests script run on
+if [ -z "$ADTTMP" ]
+then
+        echo "Required envvar \"$ADTTMP\"is not set" >&2
+        exit 1
+fi
+
+cd $ADTTMP
+
+echo "====Start whichman===="
+
+WHICHMAN_LOG=whichman.log
+
+whichman whichman > $WHICHMAN_LOG
+
+ftwhich whichman > $WHICHMAN_LOG
+
+TEST_FILE=testing.txt
+
+touch $TEST_FILE
+
+ftff $TEST_FILE > $WHICHMAN_LOG
+
+echo  "PASS"
+
+echo "====Done===="
